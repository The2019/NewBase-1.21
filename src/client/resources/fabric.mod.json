{
	"schemaVersion": 1,
	"id": "newbase",
	"version": "${version}",
	"name": "NewBase",
	"description": "A client sided utility mod for Minecraft 1.21.1 for fabric.",
	"authors": [
		"The2019"
	],
	"contact": {
		"homepage": "https://github.com/The2019/",
		"sources": "https://github.com/The2019/NewBase-1.21/"
	},
	"license": "CC0-1.0",
	"icon": "assets/newbase/icon.png",
	"environment": "client",
	"entrypoints": {
		"client": [
			"net.The2019.NewBase.NewBaseClient"
		],
		"modmenu": [ "net.The2019.NewBase.modMenu.ModMenu" ]
	},
	"mixins": [
		{
			"config": "newbase.mixins.json",
			"environment": "client"
		}
	],
	"depends": {
		"fabricloader": ">=0.16.9",
		"minecraft": "~1.21.1",
		"java": ">=21",
		"fabric-api": "*"
	},
	"suggests": {
		"modmenu": ">=11.0.3"
	}
}